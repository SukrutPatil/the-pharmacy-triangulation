    <link rel="stylesheet" href="../public/css/membershipBuying.css" />
    <link
      href="https://fonts.googleapis.com/css2?family=Baloo+Tamma+2:wght@500&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Baloo+Tammudu+2:wght@800&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <div class="nav-container">
      <nav>
        <div class="logo">
          <img src="" alt="" />
          <h1>LOGO</h1>
        </div>
        <ul class="nav-links">
          <li>
            <a href="/user/MedicationCounseling">Medication counseling</a>
          </li>
          <li>
            <a href="/user/EntrepreneurshipCounseling"></a>
              Entrepreneurship counseling
            </a>
          </li>
          <li><a href="/user/CareerCounseling">Career counseling</a></li>
          <li><a href="/user/DietCounseling">Diet counseling</a></li>
          <li><a href="/user/Products">Products</a></li>
          <li><a href="/user/Articles">Articles</a></li>
          <li><a href="/user/Articles">Dashboard</a></li>
        </ul>
        <div class="burger">
          <div class="line1"></div>
          <div class="line2"></div>
          <div class="line3"></div>
        </div>
      </nav>
    </div>
    <section>
      <h1>Get Membership</h1>

      <div class="block">
        <form action="user/membershipBuying" method="POST">
          <div class="field">
            <p>Name</p>
            <input type="text" />
          </div>
          <div class="field">
            <p>Email</p>
            <input type="text" />
          </div>
          <div class="field">
            <p>Phone No.</p>
            <input type="text" />
          </div>
          <div class="field">
            <p>Password</p>
            <input type="text" />
          </div>
          <div class="field">
            <p>Confirm Password</p>
            <input type="password" />
          </div>
          <div class="field">
            <p>Type</p>
            <select name="cars" id="cars">
              <option value="Medication counseling">All</option>
              <option value="Medication counseling">Medication counseling</option>
              <option value="Medication counseling">Diet counseling</option>
              <option value="Entrepreneurship counseling">Entrepreneurship counseling</option>
              <option value="Career counseling">Career counseling</option>
            </select>
          </div>
          <button class="logInbtn" type="submit"><p> Log In</p><img src="../public/assets/frontArrow.png" alt="" /></button>
        </form>
        <a href="">Already a member? Log In</a>
      </div>

    </section>

    <script src="../public/js/nav.js"></script>
  </body>
